{
    "name": "innoflash/za-sms",
    "description": "An SMS package to handle SMS sending from South African providers",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel SMS package",
        "South Africa SMS",
        "ZA SMS package",
        "Zoomconnect",
        "WinSMS",
        "WinSMS package"
    ],
    "authors": [
        {
            "name": "Innocent Mazando",
            "email": "innocentmazando@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/notifications": "^5.1|^6.0",
        "illuminate/support": "^5.1|^6.0",
        "illuminate/events": "^5.1|^6.0",
        "illuminate/queue": "^5.1|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Innoflash\\ZaSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Innoflash\\ZaSms\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Innoflash\\ZaSms\\ZaSmsServiceProvider"
            ],
            "aliases": {
                "Innoflash": "Innoflash\\ZaSms\\ZaSmsFacade"
            }
        }
    }
}