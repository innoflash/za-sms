{
    "name": "innoflash/za-sms",
    "description": "Your Package Description here",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Innocent Mazando",
            "email": "innocentmazando@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable":true,
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Innoflash\\ZaSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Innoflash\\ZaSms\\Tests\\":"tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Innoflash\\ZaSms\\ZaSmsServiceProvider"
            ],
            "aliases": {
                "Innoflash": "Innoflash\\ZaSms\\ZaSmsFacade"
            }
        }
    }
}